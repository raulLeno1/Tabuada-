#include <stdio.h>
#include <stdlib.h>

int main()
{
	int x;

	printf("Digite um valor\n");
	scanf("%i", &x);
	
	printf("Multiplicacao do seu valor\n");
	printf("*1=%i\n", x*1);
	printf("*2=%i\n", x*2);
	printf("*3=%i\n", x*3);
	printf("*4=%i\n", x*4);
	printf("*5=%i\n", x*5);
	printf("*6=%i\n", x*6);
	printf("*7=%i\n", x*7);
	printf("*8=%i\n", x*8);
	printf("*9=%i\n", x*9);
	printf("*10=%i\n", x*10);
	
	return 0;
}
